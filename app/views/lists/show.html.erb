<h1><%= @list.name %></h1>
<br>
<ul class="list-group mb-3">
  <li class="list-group-item">
    <% @list.movie.each do |l| %>
      <% bkm = Bookmark.find_by(list: @list, movie: l) %>
      <%= l.title %> <br> <%= bkm.comment %> <br><br>
    </li>
  <% end %>
</ul>
</ul>
<%= link_to "Volver", lists_path %>
<br>
<%= link_to "Agregar", new_list_bookmark_path(@list) %>
